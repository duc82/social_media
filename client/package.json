  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:https": "next dev --experimental-https --experimental-https-key ./certificates/key.pem --experimental-https-cert ./certificates/cert.pem --turbo",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev --turbo",
